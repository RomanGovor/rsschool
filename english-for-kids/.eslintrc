{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "no-new": "warn",
    "import/prefer-default-export": "off",
    "no-param-reassign": "off"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["airbnb"]
}
